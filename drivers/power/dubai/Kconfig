menu "Huawei Device Usage Big-data Analytics Initiative Support"

config POWER_DUBAI
	bool "Device Usage Big-data Analytics Initiative Support"
	default n
	select HUAWEI_DUBAI
	help
	  This driver provides DUBAI feature

config HUAWEI_DUBAI
	bool "Huawei Device Usage Big-data Analytics Initiative Support"
	default n
	help
	  This driver provides DUBAI feature

config POWER_DUBAI_TASK_CPU_POWER
	bool "DUBAI Task CPU Power"
	default n
	depends on POWER_DUBAI && (CPU_FREQ_POWER_STAT || ARCH_MSM)
	help
	  Get task cpu power

config HUAWEI_DUBAI_TASK_CPU_POWER
	bool "Huawei DUBAI Task CPU Power"
	default n
	depends on HUAWEI_DUBAI && CPU_FREQ_POWER_STAT
	help
	  Get task cpu power

config POWER_DUBAI_RGB_STATS
	bool "DUBAI Rgb Stats"
	default n
	depends on POWER_DUBAI && FB
	help
	  Get Rgb Stats

config HUAWEI_DUBAI_RGB_STATS
	bool "Huawei DUBAI Rgb Stats"
	default n
	depends on HUAWEI_DUBAI && FB
	help
	  Get Rgb Stats

endmenu
