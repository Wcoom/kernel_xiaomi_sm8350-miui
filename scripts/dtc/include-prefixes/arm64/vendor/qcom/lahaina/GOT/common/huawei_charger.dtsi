			input-enable;
		};
	};

	wireless_aux_tx_kb_pins_default: wireless_aux_tx_kb_pins_default {
		mux {
			pins = "gpio78";
			function = "gpio";
		};
		config {
			pins = "gpio78";
			drive-strength = <2>;
			bias-pull-down;
			input-enable;
		};
	};

	battct_swi_pins_default: battct_swi_pins_default {
		mux {
			pins = "gpio155";
			drive-strength = <6>;
			function = "gpio";
		};
		config {
			pins = "gpio155";
			input-enable;
		};
	};
