		rgltr-max-voltage = <0>;
		rgltr-load-current = <0>;
		gpio-no-mux = <0>;
		pinctrl-names = "cam_default", "cam_suspend";
		pinctrl-0 = <&mcam_lowPowerVA_active>;
		pinctrl-1 = <&mcam_lowPowerVA_suspend>;
		gpios = <&tlmm 61 0>, /* mcam_vavdd (afvdd) gpio61 */
			<&pm8350c_gpios 7 0>; /* lowPowerVA 8350c(7350c) gpio 7 */
		gpio-mixed1 = <0>;
