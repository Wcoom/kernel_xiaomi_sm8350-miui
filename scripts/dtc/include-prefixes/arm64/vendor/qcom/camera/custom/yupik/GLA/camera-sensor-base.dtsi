		config {
			pins = "gpio102";
			bias-pull-down; /* PULL DOWN */
			drive-strength = <2>; /* 2 MA */
			output-low;
		};
	};

	cam_sensor_active_pwdn4: cam_sensor_active_pwdn4 {
		/* PWDN 4 */
		mux {
			pins = "gpio96";
			function = "gpio";
		};

		config {
			pins = "gpio96";
			bias-disable; /* No PULL */
			drive-strength = <2>; /* 2 MA */
		};
	};

	cam_sensor_suspend_pwdn4: cam_sensor_suspend_pwdn4 {
		/* PWDN 4 */
		mux {
			pins = "gpio96";
