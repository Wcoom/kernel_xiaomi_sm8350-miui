#include <dt-bindings/clock/qcom,camcc-yupik.h>

&product_name_camera {
	product-name = "gla";
	led-dmd-enable = <1>;
};

&pm8350c_flash0 {
	qcom,duration-ms = <250>;
};

&pm8350c_flash1 {
	qcom,duration-ms = <250>;
};

&pm8350c_flash2 {
	qcom,duration-ms = <250>;
};

&pm8350c_flash3 {
	qcom,duration-ms = <250>;
};

&soc {
	qcom,cam-res-mgr {
		compatible = "qcom,cam-res-mgr";
		gpios-shared-pinctrl = <474 337 466 340 403>; /* 7315gpio offset is 336 */
		shared-pctrl-gpio-names = "mcam_avdd", "mcam_gpio1_iovdd", "mcam_dvdd", "mcam_afvdd", "cam_sensor_mclk3";
		pinctrl-names = "mcam_avdd_active", "mcam_avdd_suspend",
			"mcam_gpio1_iovdd_active", "mcam_gpio1_iovdd_suspend",
			"mcam_dvdd_active", "mcam_dvdd_suspend",
			"mcam_afvdd_active", "mcam_afvdd_suspend",
			"cam_sensor_mclk3_active", "cam_sensor_mclk3_suspend";
		pinctrl-0 = <&mcam_avdd_active>;
		pinctrl-1 = <&mcam_avdd_suspend>;
		pinctrl-2 = <&mcam_gpio1_iovdd_active>;
		pinctrl-3 = <&mcam_gpio1_iovdd_suspend>;
		pinctrl-4 = <&mcam_dvdd_active>;
		pinctrl-5 = <&mcam_dvdd_suspend>;
		pinctrl-6 = <&mcam_afvdd_active>;
		pinctrl-7 = <&mcam_afvdd_suspend>;
		pinctrl-8 = <&cam_sensor_mclk3_active>;
		pinctrl-9 = <&cam_sensor_mclk3_suspend>;
		status = "ok";
	};

	led_flash_triple_rear: qcom,camera-flash@4 {
		cell-index = <4>;
		compatible = "qcom,camera-flash";
		flash-source = <&pm8350c_flash0>;
		torch-source = <&pm8350c_torch0>;
		switch-source = <&pm8350c_switch2>;
		torch-light-num = <0>;
		torch-light-name = "torch-light0";
		status = "ok";
	};

	led_flash_rear_wide: qcom,camera-flash@6 {
		cell-index = <6>;
		compatible = "qcom,camera-flash";
		flash-source = <&pm8350c_flash0>;
		torch-source = <&pm8350c_torch0>;
		switch-source = <&pm8350c_switch2>;
		status = "ok";
	};

	qcom,camcfgdev {
		compatible = "qcom,camcfgdev";
		status = "ok";
	};
};

&qupv3_se1_i2c {
	status = "disabled";
};

&pm8350c_gpios {
	mcam_ponv_active: mcam_ponv_active {
		pins = "gpio4";
		function = "normal";
		power-source = <0>;
		output-high;
		bias-disable;
	};

	mcam_ponv_suspend: mcam_ponv_suspend {
		pins = "gpio4";
		function = "normal";
		power-source = <0>;
		output-low;
		bias-disable;
	};
};

&tlmm {
	mcam_avdd_active: mcam_avdd_active {
		/* mcam_avdd */
		mux {
			pins = "gpio138";
			function = "gpio";
		};

		config {
			pins = "gpio138";
			bias-disable; /* No PULL */
			drive-strength = <2>; /* 2 MA */
		};
	};

	mcam_avdd_suspend: mcam_avdd_suspend {
		/* mcam_avdd */
		mux {
			pins = "gpio138";
			function = "gpio";
		};

		config {
			pins = "gpio138";
			bias-pull-down; /* PULL DOWN */
			drive-strength = <2>; /* 2 MA */
			output-low;
		};
	};

	rear_main_mcam_avdd_active: rear_main_mcam_avdd_active {
		/* rear_main_mcam_avdd_1v8 */
		mux {
			pins = "gpio68";
			function = "gpio";
		};

		config {
			pins = "gpio68";
			bias-pull-down; /* PULL DOWN */
			drive-strength = <2>; /* 2 MA */
		};
	};

	rear_main_mcam_avdd_suspend: rear_main_mcam_avdd_suspend {
		/* rear_main_mcam_avdd_1v8 */
		mux {
			pins = "gpio68";
