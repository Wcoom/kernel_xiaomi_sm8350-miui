		regulator-names = "cam_vaf", "cam_clk";
		rgltr-cntrl-support;
		rgltr-min-voltage = <3100000 0>;
		rgltr-max-voltage = <3150000 0>;
		rgltr-load-current = <0 0>; /* value? */
		gpio-no-mux = <0>;
		pinctrl-names = "cam_default", "cam_suspend";
		pinctrl-0 = <&cam_sensor_mclk2_active
				&cam_sensor_active_rst2
				&rear_main_mcam_avdd_active>;
		pinctrl-1 = <&cam_sensor_mclk2_suspend
				&cam_sensor_suspend_rst2
				&rear_main_mcam_avdd_suspend>;
		gpios = <&tlmm 66 0>, /* mclk2 gpio 66 */
			<&tlmm 77 0>, /* reset2 gpio 77 */
			<&tlmm 138 0>, /*rear_mcam_avdd 2.85v gpio 138 */
			<&tlmm 68 0>, /*rear_mcam_avdd 1.8v gpio 68 */
			<&tlmm 1 0>,  /*mcam_iovdd gpio 1 */
			<&tlmm 130 0>, /*mcam_dvdd gpio 130 */
			<&tlmm 4 0>; /* afvdd 7315 gpio 4 */
		gpio-reset = <1>;
		gpio-mixed1 = <2>;
		gpio-mixed2 = <3>;
		gpio-mixed3 = <4>;
		gpio-mixed4 = <5>;
		gpio-mixed5 = <6>;
		gpio-req-tbl-num = <0 1 2 3 4 5 6>;
		gpio-req-tbl-flags = <1 0 0 0 0 0 0>;
		gpio-req-tbl-label = "CAMIF_MCLK2",
