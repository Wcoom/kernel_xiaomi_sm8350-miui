		cam_vaf-supply = <&pm8350c_l11>; /* afvdd pm8350c l11 3.1v */
		cam_clk-supply = <&cam_cc_titan_top_gdsc>;
		regulator-names = "cam_vaf", "cam_clk";
		rgltr-cntrl-support;
		rgltr-min-voltage = <3100000 0>;
		rgltr-max-voltage = <3150000 0>;
		rgltr-load-current = <0 0>; /* value? */
		gpio-no-mux = <0>;
		pinctrl-names = "cam_default", "cam_suspend";
		pinctrl-0 = <&cam_sensor_mclk1_active
