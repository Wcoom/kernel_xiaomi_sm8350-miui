		regulator-names = "cam_clk";
		rgltr-cntrl-support;
		rgltr-min-voltage = <0>;
		rgltr-max-voltage = <0>;
		rgltr-load-current = <0>;
		gpio-no-mux = <0>;
		gpios = <&tlmm 65 0>, /* mclk1 gpio 65 */
			<&tlmm 1 0>;  /*mcam_iovdd gpio 1 */
		gpio-mixed1 = <1>;
		gpio-req-tbl-num = <0 1>;
		gpio-req-tbl-flags = <1 0>;
		gpio-req-tbl-label = "CAMIF_MCLK1",
					"CAM_MIXED1";
		cci-master = <1>; /* i2c1 */
		status = "ok";
		clocks = <&camcc CAM_CC_MCLK1_CLK>;
		clock-names = "cam_clk";
		clock-cntl-level = "nominal";
		clock-rates = <19200000>;
	};

	/* front tel actuator */
	actuator_front_tel: qcom,actuator7 {
		cell-index = <7>;
		compatible = "qcom,actuator";
		status = "ok";
		sensor-mixed-pin = <0>;
