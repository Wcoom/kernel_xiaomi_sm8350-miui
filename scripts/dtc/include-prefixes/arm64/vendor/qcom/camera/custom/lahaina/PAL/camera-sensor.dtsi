#include <dt-bindings/clock/qcom,camcc-lahaina.h>

&soc {
	led_flash_triple_rear: qcom,camera-flash@4 {
		cell-index = <4>;
		compatible = "qcom,camera-flash";
		flash-source = <&pm8350c_flash0 &pm8350c_flash1>;
		torch-source = <&pm8350c_torch0 &pm8350c_torch1>;
		switch-source = <&pm8350c_switch0>;
		torch-light-num = <0 1>;
		torch-light-name = "torch-light0", "torch-light1";
		status = "ok";
	};

	led_flash_triple_rear_aux: qcom,camera-flash@5 {
		cell-index = <5>;
		compatible = "qcom,camera-flash";
		flash-source = <&pm8350c_flash0 &pm8350c_flash1>;
		torch-source = <&pm8350c_torch0 &pm8350c_torch1>;
		switch-source = <&pm8350c_switch0>;
		status = "ok";
	};

	led_flash_triple_rear_aux2: qcom,camera-flash@6 {
		cell-index = <6>;
		compatible = "qcom,camera-flash";
		flash-source = <&pm8350c_flash0 &pm8350c_flash1>;
		torch-source = <&pm8350c_torch0 &pm8350c_torch1>;
		switch-source = <&pm8350c_switch0>;
		status = "ok";
	};

	qcom,cam-res-mgr {
		compatible = "qcom,cam-res-mgr";
		status = "ok";
		gpios-shared-pinctrl = <333 345 382 352 392>;
		shared-pctrl-gpio-names = "mcam_iovdd", "mcam_dvdd", "mcam_afvdd", "mcam_avdd", "tcam_drvdd";
		pinctrl-names = "mcam_iovdd_active", "mcam_iovdd_suspend", "mcam_dvdd_active",
				"mcam_dvdd_suspend", "mcam_afvdd_active", "mcam_afvdd_suspend",
				"mcam_avdd_active", "mcam_avdd_suspend", "tcam_drvdd_active", "tcam_drvdd_suspend";
		pinctrl-0 = <&mcam_iovdd_active>;
		pinctrl-1 = <&mcam_iovdd_suspend>;
		pinctrl-2 = <&mcam_dvdd_active>;
		pinctrl-3 = <&mcam_dvdd_suspend>;
		pinctrl-4 = <&mcam_afvdd_active>;
		pinctrl-5 = <&mcam_afvdd_suspend>;
		pinctrl-6 = <&mcam_avdd_active>;
		pinctrl-7 = <&mcam_avdd_suspend>;
		pinctrl-8 = <&tcam_drvdd_active>;
		pinctrl-9 = <&tcam_drvdd_suspend>;
	};

	qcom,camcfgdev {
		compatible = "qcom,camcfgdev";
		status = "ok";
	};
};

&product_name_camera {
    product-name = "palau";
};

