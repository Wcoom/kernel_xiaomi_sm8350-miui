#include <dt-bindings/clock/qcom,camcc-yupik.h>

&product_name_camera {
	product-name = "ban";
};

&soc {
	led_flash_triple_rear: qcom,camera-flash@4 {
		cell-index = <4>;
		compatible = "qcom,camera-flash";
		flash-source = <&pm8350c_flash0 &pm8350c_flash1>;
		torch-source = <&pm8350c_torch0 &pm8350c_torch1>;
		switch-source = <&pm8350c_switch2>;
		torch-light-num = <0 1>;
		torch-light-name = "torch-light0", "torch-light1";
		status = "ok";
	};

	led_flash_triple_wide: qcom,camera-flash@6 {
		cell-index = <6>;
		compatible = "qcom,camera-flash";
		flash-source = <&pm8350c_flash0 &pm8350c_flash1>;
		torch-source = <&pm8350c_torch0 &pm8350c_torch1>;
		switch-source = <&pm8350c_switch2>;
		status = "ok";
	};

	qcom,cam-res-mgr {
		compatible = "qcom,cam-res-mgr";
		gpios-shared-pinctrl = <492 488 477 458 491>; /* gpio156 gpio152 gpio141 gpio122 gpio155 */
		shared-pctrl-gpio-names = "mcam_iovdd", "mcam_avdd1_8", "mcam_afvdd", "mcam_dvdd", "mcam_avdd2_85";
		pinctrl-names = "mcam_iovdd_active", "mcam_iovdd_suspend",
				"mcam_avdd1_8_active", "mcam_avdd1_8_suspend",
				"mcam_afvdd_active", "mcam_afvdd_suspend",
				"mcam_dvdd_active", "mcam_dvdd_suspend",
				"mcam_avdd2_85_active", "mcam_avdd2_85_suspend";
		pinctrl-0 = <&mcam_iovdd_active>;
		pinctrl-1 = <&mcam_iovdd_suspend>;
		pinctrl-2 = <&mcam_avdd1_8_active>;
		pinctrl-3 = <&mcam_avdd1_8_suspend>;
		pinctrl-4 = <&mcam_afvdd_active>;
		pinctrl-5 = <&mcam_afvdd_suspend>;
		pinctrl-6 = <&mcam_dvdd_active>;
		pinctrl-7 = <&mcam_dvdd_suspend>;
		pinctrl-8 = <&mcam_avdd2_85_active>;
		pinctrl-9 = <&mcam_avdd2_85_suspend>;
		status = "ok";
	};

	qcom,camcfgdev {
		compatible = "qcom,camcfgdev";
		status = "ok";
	};
};

