			<&tlmm 3 0>, /* pwdn4 gpio 3 */
			<&tlmm 0 0>; /* mcam_iovdd gpio 0 1.8v */
		gpio-reset = <1>;
		gpio-custom1 = <2>;
		gpio-mixed1 = <3>;
		gpio-req-tbl-num = <0 1 2 3>;
		gpio-req-tbl-flags = <1 0 0 0>;
		gpio-req-tbl-label = "CAMIF_MCLK1",
					"CAM_RESET4",
					"CAM_PWDN4",
					"CAM_MIXED1";
		cci-master = <1>; /* i2c3 */
		status = "ok";
