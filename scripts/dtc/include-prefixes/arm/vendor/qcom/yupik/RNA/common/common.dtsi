&gpio_check {
	/* 343 : GPIO7  , tlmm BASE = 336 */
	/* 486 : GPIO150, tlmm BASE = 336 */
	/* 487 : GPIO151, tlmm BASE = 336 */
	/* 489 : GPIO153, tlmm BASE = 336 */
	/* 477 : GPIO141, tlmm BASE = 336 */
	/* 379 : GPIO43 , tlmm BASE = 336 */
	buckle_num = "343 486 487 489 477 379";
	buckle_val = "0 0 0 0 0 0";
};

&als_after_sale_tsl2591 {
	compatible = "tsl2591";
	als_after_sale_product = <22>;
	als_after_sale_xl = <798>;
	als_after_sale_yl = <2>;
	als_after_sale_xr = <898>;
	als_after_sale_yr = <102>;
	als_after_sale_gamma = <0>;
};

&als_after_sale_ltr311 {
	compatible = "ltr311";
	als_after_sale_product = <23>;
	als_after_sale_xl = <798>;
	als_after_sale_yl = <2>;
	als_after_sale_xr = <898>;
	als_after_sale_yr = <102>;
